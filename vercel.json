{
  "version": 2,
  "name": "esp32-anti-theft-dashboard",
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/dashboard/$1"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=3600"
        }
      ]
    }
  ]
}